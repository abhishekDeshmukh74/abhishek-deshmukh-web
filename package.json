{
  "name": "abhishek-deshmukh-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng s -o",
    "start": "npm run serve:ssr",
    "build": "npm run build:ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dyno:hours": "heroku ps --app abhishek-deshmukh-frontend",
    "heroku-logs": "heroku logs -t --app abhishek-deshmukh-frontend",
    "lint": "tslint --fix \"src/**/*.ts\"",
    "npm:update": "npm update",
    "stats": "ng build --configuration production --named-chunks --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "http:serve": "http-server -p 8080 -c-1 dist/browser",
    "dev:ssr": "ng run abhishek-deshmukh-frontend:serve-ssr",
    "serve:ssr": "node dist/abhishek-deshmukh-frontend/server/main.js",
    "build:ssr": "ng build --configuration production && ng run abhishek-deshmukh-frontend:server",
    "prerender": "ng run abhishek-deshmukh-frontend:prerender",
    "backend": "cd ../abhishek-deshmukh-api && node dist/server",
    "backend-logs": "cd ../abhishek-deshmukh-api && npm run heroku-logs",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "engines": {
    "node": "18.15.0",
    "npm": "9.6.4"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "~15.2.6",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "~15.2.6",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/platform-server": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@angular/service-worker": "^15.2.0",
    "@nguniversal/express-engine": "^15.2.1",
    "@sentry/browser": "^7.47.0",
    "angular-mentions": "^1.5.0",
    "express": "^4.15.2",
    "material-icons-font": "^2.1.0",
    "ng2-adsense": "^12.0.0",
    "ngx-highlightjs": "^8.0.0",
    "rxjs": "~7.8.0",
    "storage-factory": "^0.2.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
