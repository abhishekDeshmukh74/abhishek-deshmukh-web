{
  "name": "abhishek-deshmukh-frontend",
  "version": "0.0.0",
  "scripts": {
    "serve": "ng s -o",
    "start": "npm run serve:ssr",
    "build": "npm run build:ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "tslint --fix \"src/**/*.ts\"",
    "npm:update": "npm update",
    "stats": "ng build --configuration production --named-chunks --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "http:serve": "http-server -p 8080 -c-1 dist/browser",
    "dev:ssr": "ng run abhishek-deshmukh-frontend:serve-ssr",
    "serve:ssr": "node dist/abhishek-deshmukh-frontend/server/main.js",
    "build:ssr": "ng build --configuration production && ng run abhishek-deshmukh-frontend:server",
    "prerender": "ng run abhishek-deshmukh-frontend:prerender",
    "backend": "cd ../abhishek-deshmukh-api && node dist/server",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "engines": {
    "node": "24.x",
    "npm": "10.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@angular/ssr": "^21.0.0",
    "@sentry/browser": "^7.47.0",
    "@trademe/ng-defer-load": "^14.1.0",
    "angular-mentions": "^1.5.0",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "helmet": "^6.1.5",
    "material-icons-font": "^2.1.0",
    "ng2-adsense": "^12.0.0",
    "ngx-highlightjs": "^8.0.0",
    "rxjs": "~7.8.0",
    "storage-factory": "^0.2.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.9.3"
  }
}
